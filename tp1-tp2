	end)
end

---------------------------------------------------------------------
-- ğŸ”¸ PhÃ¢n nhÃ¡nh tÃ i khoáº£n
if name == "urydyo1" then
	print("[MULTI TELEPORT] ğŸšª KÃ­ch hoáº¡t Auto Teleport Cá»•ng 1 cho urydyo1")
	task.spawn(autoTeleportGate1)
elseif name == "ikkopolconloli" then
	print("[MULTI TELEPORT] ğŸšª KÃ­ch hoáº¡t Auto Teleport Cá»•ng 2 cho ikkopolconloli")
	task.spawn(autoTeleportGate2)
else
	warn("[MULTI TELEPORT] âš ï¸ KhÃ´ng cÃ³ nhiá»‡m vá»¥ cho tÃ i khoáº£n:", name)
end
