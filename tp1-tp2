-- ğŸŒ Multi Auto Teleport (Cá»•ng 1 & Cá»•ng 2)
-- TÃ¡c giáº£: Hien | PhiÃªn báº£n gá»™p dÃ nh cho 2 tÃ i khoáº£n

local Players = game:GetService("Players")
local p = Players.LocalPlayer or Players.PlayerAdded:Wait()
local name = p.Name
print("[MULTI TELEPORT] ğŸ‘¤ Player:", name)

---------------------------------------------------------------------
-- ğŸ”¹ HÃ m tiá»‡n Ã­ch chung: chá» HumanoidRootPart
local function waitForHRP()
	local chr = p.Character or p.CharacterAdded:Wait()
	local hrp = chr:FindFirstChild("HumanoidRootPart")
	local waited = 0
	while not hrp and waited < 5 do
		task.wait(0.2)
		waited += 0.2
		chr = p.Character or p.CharacterAdded:Wait()
		hrp = chr:FindFirstChild("HumanoidRootPart")
	end
	return hrp
end

---------------------------------------------------------------------
-- ğŸŒ€ Script 1 â€” Auto Teleport Cá»•ng 1 (cho urydyo1)
local function autoTeleportGate1()
	local teleportPos = Vector3.new(18.86713409423828, 0.37274569272994995, -114.86202239990234)

	local function teleportToGate()
		local hrp = waitForHRP()
		if hrp then
			pcall(function()
				hrp.CFrame = CFrame.new(teleportPos + Vector3.new(0, 5, 0))
			end)
			print("[AUTO] âœ¨ ÄÃ£ dá»‹ch chuyá»ƒn tá»›i Cá»•ng 1 táº¡i:", os.date("%X"))
		else
			warn("[AUTO] âš ï¸ KhÃ´ng tÃ¬m tháº¥y HumanoidRootPart â€” khÃ´ng thá»ƒ dá»‹ch chuyá»ƒn.")
		end
	end

	task.spawn(function()
		print("[AUTO] â³ Báº¯t Ä‘áº§u chuá»—i Auto Teleport Cá»•ng 1...")
		task.wait(3)
		print("[AUTO] ğŸš€ Dá»‹ch chuyá»ƒn tá»›i Cá»•ng 1 (Láº§n 1 - sau 3s)")
		teleportToGate()

		task.wait(60)
		print("[AUTO] ğŸš€ Dá»‹ch chuyá»ƒn láº¡i tá»›i Cá»•ng 1 (Láº§n 2 - sau 63s)")
		teleportToGate()

		task.wait(50)
		print("[AUTO] ğŸš€ Dá»‹ch chuyá»ƒn láº§n 3 tá»›i Cá»•ng 1 (sau 113s)")
		teleportToGate()

		print("[AUTO] âœ… HoÃ n táº¥t chuá»—i dá»‹ch chuyá»ƒn 3 láº§n Cá»•ng 1.")
	end)
end

---------------------------------------------------------------------
-- ğŸŒ€ Script 2 â€” Auto Teleport Cá»•ng 2 (cho ikkopolconloli)
local function autoTeleportGate2()
	local teleportPos = Vector3.new(18.37334884643555, 0.37274569272994995, -140.72775268554688)

	local function teleportToGate()
		local hrp = waitForHRP()
		if hrp then
			pcall(function()
				hrp.CFrame = CFrame.new(teleportPos + Vector3.new(0, 5, 0))
			end)
			print("[AUTO] âœ¨ ÄÃ£ dá»‹ch chuyá»ƒn tá»›i Cá»•ng 2 táº¡i:", os.date("%X"))
		else
			warn("[AUTO] âš ï¸ KhÃ´ng tÃ¬m tháº¥y HumanoidRootPart â€” khÃ´ng thá»ƒ dá»‹ch chuyá»ƒn.")
		end
	end

	task.spawn(function()
		print("[AUTO] â³ Báº¯t Ä‘áº§u chuá»—i Auto Teleport Cá»•ng 2...")
		task.wait(3)
		print("[AUTO] ğŸš€ Dá»‹ch chuyá»ƒn tá»›i Cá»•ng 2 (Láº§n 1 - sau 3s)")
