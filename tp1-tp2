-- üåê Multi Auto Teleport (C·ªïng 1 & C·ªïng 2)
-- T√°c gi·∫£: Hien | B·∫£n g·ªôp d√†nh cho 2 t√†i kho·∫£n (ƒë√£ ch·ªânh th·ªùi gian 3s - 80s - 156s)

local Players = game:GetService("Players")
local p = Players.LocalPlayer or Players.PlayerAdded:Wait()
local name = p.Name
print("[MULTI TELEPORT] üë§ Player:", name)

---------------------------------------------------------------------
-- üîπ H√†m ti·ªán √≠ch chung: ch·ªù HumanoidRootPart
local function waitForHRP()
	local chr = p.Character or p.CharacterAdded:Wait()
	local hrp = chr:FindFirstChild("HumanoidRootPart")
	local waited = 0
	while not hrp and waited < 5 do
		task.wait(0.2)
		waited += 0.2
		chr = p.Character or p.CharacterAdded:Wait()
		hrp = chr:FindFirstChild("HumanoidRootPart")
	end
	return hrp
end

---------------------------------------------------------------------
-- üåÄ Script 1 ‚Äî Auto Teleport C·ªïng 1 (cho urydyo1)
local function autoTeleportGate1()
	local teleportPos = Vector3.new(18.86713409423828, 0.37274569272994995, -114.86202239990234)

	local function teleportToGate()
		local hrp = waitForHRP()
		if hrp then
			pcall(function()
				hrp.CFrame = CFrame.new(teleportPos + Vector3.new(0, 5, 0))
			end)
			print("[AUTO] ‚ú® ƒê√£ d·ªãch chuy·ªÉn t·ªõi C·ªïng 1 t·∫°i:", os.date("%X"))
		else
			warn("[AUTO] ‚ö†Ô∏è Kh√¥ng t√¨m th·∫•y HumanoidRootPart ‚Äî kh√¥ng th·ªÉ d·ªãch chuy·ªÉn.")
		end
	end

	task.spawn(function()
		print("[AUTO] ‚è≥ B·∫Øt ƒë·∫ßu chu·ªói Auto Teleport C·ªïng 1...")

		-- üîπ L·∫ßn 1: sau 3s
		task.wait(3)
		print("[AUTO] üöÄ D·ªãch chuy·ªÉn t·ªõi C·ªïng 1 (L·∫ßn 1 - sau 3s)")
		teleportToGate()

		-- üîπ L·∫ßn 2: sau th√™m 77s (t·ªïng 80s)
		task.wait(77)
		print("[AUTO] üöÄ D·ªãch chuy·ªÉn l·∫°i t·ªõi C·ªïng 1 (L·∫ßn 2 - sau 80s)")
		teleportToGate()

		-- üîπ L·∫ßn 3: sau th√™m 76s (t·ªïng 156s)
		task.wait(76)
		print("[AUTO] üöÄ D·ªãch chuy·ªÉn l·∫ßn 3 t·ªõi C·ªïng 1 (sau 156s)")
		teleportToGate()

		print("[AUTO] ‚úÖ Ho√†n t·∫•t chu·ªói d·ªãch chuy·ªÉn 3 l·∫ßn C·ªïng 1.")
	end)
end

---------------------------------------------------------------------
-- üåÄ Script 2 ‚Äî Auto Teleport C·ªïng 2 (cho ikkopolconloli)
local function autoTeleportGate2()
	local teleportPos = Vector3.new(18.37334884643555, 0.37274569272994995, -140.72775268554688)

	local function teleportToGate()
		local hrp = waitForHRP()
		if hrp then
			pcall(function()
				hrp.CFrame = CFrame.new(teleportPos + Vector3.new(0, 5, 0))
			end)
			print("[AUTO] ‚ú® ƒê√£ d·ªãch chuy·ªÉn t·ªõi C·ªïng 2 t·∫°i:", os.date("%X"))
		else
			warn("[AUTO] ‚ö†Ô∏è Kh√¥ng t√¨m th·∫•y HumanoidRootPart ‚Äî kh√¥ng th·ªÉ d·ªãch chuy·ªÉn.")
		end
	end

	task.spawn(function()
		print("[AUTO] ‚è≥ B·∫Øt ƒë·∫ßu chu·ªói Auto Teleport C·ªïng 2...")

		-- üîπ L·∫ßn 1: sau 3s
		task.wait(3)
		print("[AUTO] üöÄ D·ªãch chuy·ªÉn t·ªõi C·ªïng 2 (L·∫ßn 1 - sau 3s)")
		teleportToGate()

		-- üîπ L·∫ßn 2: sau th√™m 77s (t·ªïng 80s)
		task.wait(77)
		print("[AUTO] üöÄ D·ªãch chuy·ªÉn l·∫°i t·ªõi C·ªïng 2 (L·∫ßn 2 - sau 80s)")
		teleportToGate()

		-- üîπ L·∫ßn 3: sau th√™m 76s (t·ªïng 156s)
		task.wait(76)
		print("[AUTO] üöÄ D·ªãch chuy·ªÉn l·∫ßn 3 t·ªõi C·ªïng 2 (sau 156s)")
		teleportToGate()

		print("[AUTO] ‚úÖ Ho√†n t·∫•t chu·ªói d·ªãch chuy·ªÉn 3 l·∫ßn C·ªïng 2.")
	end)
end

---------------------------------------------------------------------
-- üî∏ Ph√¢n nh√°nh t√†i kho·∫£n
if name == "urydyo1" then
	print("[MULTI TELEPORT] üö™ K√≠ch ho·∫°t Auto Teleport C·ªïng 1 cho urydyo1")
	task.spawn(autoTeleportGate1)
elseif name == "ikkopolconloli" then
	print("[MULTI TELEPORT] üö™ K√≠ch ho·∫°t Auto Teleport C·ªïng 2 cho ikkopolconloli")
	task.spawn(autoTeleportGate2)
else
	warn("[MULTI TELEPORT] ‚ö†Ô∏è Kh√¥ng c√≥ nhi·ªám v·ª• cho t√†i kho·∫£n:", name)
end
