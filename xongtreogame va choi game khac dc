-- HÃ m load script an toÃ n
local function safeLoad(url)
	task.spawn(function()
		local success, result = pcall(function()
			local source = game:HttpGet(url)
			if source and #source > 10 then
				print("âš™ï¸ Äang cháº¡y script:", url)
				loadstring(source)()
			else
				warn("âš ï¸ Script rá»—ng hoáº·c khÃ´ng táº£i Ä‘Æ°á»£c:", url)
			end
		end)
		if not success then
			warn("âŒ Lá»—i khi cháº¡y script:", tostring(result))
		end
	end)
end

-- Cháº¡y script tÆ°Æ¡ng á»©ng tÃ i khoáº£n
if userScripts[username] then
	print("âœ… ÄÄƒng nháº­p há»£p lá»‡:", username)
	print("ğŸ“¦ Báº¯t Ä‘áº§u táº£i script cho tÃ i khoáº£n:", username)

	for _, url in ipairs(userScripts[username]) do
		safeLoad(url)
		task.wait(0.7) -- giÃ£n thá»i gian nháº¹ trÃ¡nh lag
	end

	print("âœ¨ HoÃ n táº¥t táº£i toÃ n bá»™ script cho acc:", username)
else
